# Configuración de Base de Datos Oracle
DB_TYPE=oracle
DB_HOST=coyan.aduana.cl
DB_PORT=1522
DB_USERNAME=
DB_PASSWORD=
DB_SID=aries
DB_SCHEMA=FISCALIZACIONES

# Configuración de Oracle Instant Client
# Para Docker/Linux usar la ruta del contenedor
ORACLE_INSTANT_CLIENT_PATH=/opt/oracle/instantclient_19_3
# Para Windows local usar: C:\oracle\instantclient_21_19

# Configuración de la aplicación
PORT=3000
NODE_ENV=development

# Configuración específica de Oracle (opcional, ya están configuradas en el Dockerfile)
# ORACLE_CLIENT_LIB_DIR=/opt/oracle/instantclient_19_3
# LD_LIBRARY_PATH=/opt/oracle/instantclient_19_3
# TNS_ADMIN=/opt/oracle/instantclient_19_3
# OCI_LIB_DIR=/opt/oracle/instantclient_19_3

# Configuración de logging (opcional)
LOG_LEVEL=info

# Configuración de timeouts (opcional)
DB_CONNECTION_TIMEOUT=60000
DB_IDLE_TIMEOUT=30000